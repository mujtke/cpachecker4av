<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.9.dtd">
<benchmark tool="cpachecker" timelimit="900s" hardtimelimit="900s" cpuCores="1" threads="2">

	<!-- <rundefinition> defines tool configurations to benchmark. -->
	<rundefinition name="myAnalysis-concurrency">
		<tasks name="ConcurrencySafety">
			<includesfile>../sv-benchmarks/c/ConcurrencySafety-Main.set</includesfile>
			<option name="-config">./config/myAnalysis-concurrency.properties</option>
			<option name="-spec">./config/specification/sv-comp-reachability.spc</option>
		</tasks>
	</rundefinition>

	<propertyfile>../sv-benchmarks/c/properties/unreach-call.prp</propertyfile>

</benchmark>
